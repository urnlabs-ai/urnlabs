<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="backgroundGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0f172a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="brandGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0ea5e9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0284c7;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="accentGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366f1;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:0.2" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="url(#backgroundGradient)" />
  
  <!-- Background pattern -->
  <pattern id="dots" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
    <circle cx="20" cy="20" r="1" fill="rgba(255,255,255,0.1)" />
  </pattern>
  <rect width="1200" height="630" fill="url(#dots)" />
  
  <!-- Decorative elements -->
  <circle cx="1000" cy="150" r="80" fill="url(#accentGradient)" />
  <circle cx="200" cy="500" r="60" fill="url(#accentGradient)" />
  
  <!-- Main content area -->
  <rect x="80" y="120" width="1040" height="390" rx="20" fill="rgba(255,255,255,0.05)" stroke="rgba(255,255,255,0.1)" stroke-width="1" />
  
  <!-- Profile avatar -->
  <circle cx="200" cy="250" r="60" fill="url(#brandGradient)" />
  <text x="200" y="265" font-family="Inter, system-ui, sans-serif" font-size="36" font-weight="bold" text-anchor="middle" fill="white">UR</text>
  
  <!-- Main heading -->
  <text x="300" y="220" font-family="Inter, system-ui, sans-serif" font-size="48" font-weight="bold" fill="white">
    Muhammad Usman Ramzan
  </text>
  
  <!-- Subtitle -->
  <text x="300" y="270" font-family="Inter, system-ui, sans-serif" font-size="28" font-weight="600" fill="#0ea5e9">
    Senior AI Engineer &amp; Platform Architect
  </text>
  
  <!-- Description -->
  <text x="300" y="320" font-family="Inter, system-ui, sans-serif" font-size="22" fill="#cbd5e1">
    Specializing in Claude Code, AI Automation, and Platform Architecture
  </text>
  
  <!-- Skills badges -->
  <rect x="300" y="350" width="140" height="35" rx="18" fill="rgba(14, 165, 233, 0.2)" stroke="#0ea5e9" stroke-width="1" />
  <text x="370" y="372" font-family="Inter, system-ui, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#0ea5e9">Claude Code</text>
  
  <rect x="460" y="350" width="120" height="35" rx="18" fill="rgba(99, 102, 241, 0.2)" stroke="#6366f1" stroke-width="1" />
  <text x="520" y="372" font-family="Inter, system-ui, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#6366f1">AI Platform</text>
  
  <rect x="600" y="350" width="130" height="35" rx="18" fill="rgba(139, 92, 246, 0.2)" stroke="#8b5cf6" stroke-width="1" />
  <text x="665" y="372" font-family="Inter, system-ui, sans-serif" font-size="16" font-weight="600" text-anchor="middle" fill="#8b5cf6">Automation</text>
  
  <!-- Website URL -->
  <text x="300" y="430" font-family="Inter, system-ui, sans-serif" font-size="20" font-weight="500" fill="#94a3b8">
    usmanramzan.ai
  </text>
  
  <!-- Bottom branding -->
  <rect x="80" y="550" width="1040" height="1" fill="rgba(255,255,255,0.1)" />
  <text x="600" y="585" font-family="Inter, system-ui, sans-serif" font-size="16" font-weight="500" text-anchor="middle" fill="#64748b">
    Building production-ready AI agent platforms that deliver measurable ROI
  </text>
</svg>
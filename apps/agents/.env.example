# Environment Configuration
NODE_ENV=development
AGENT_SERVICE_PORT=3001
HOST=localhost

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/urnlabs_dev

# Redis (Required for agent queues)
REDIS_URL=redis://localhost:6379

# AI Services
CLAUDE_API_KEY=your-claude-api-key
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Agent Configuration
AGENT_QUEUE_CONCURRENCY=5
AGENT_TASK_TIMEOUT=300
AGENT_MAX_RETRIES=3
AGENT_MEMORY_LIMIT=512

# Queue Configuration
QUEUE_REDIS_PREFIX=urnlabs:agents
QUEUE_DEFAULT_DELAY=0
QUEUE_MAX_ATTEMPTS=3
QUEUE_BACKOFF_TYPE=exponential
QUEUE_BACKOFF_DELAY=2000

# Monitoring
LOG_LEVEL=info
METRICS_ENABLED=true
PERFORMANCE_MONITORING=true

# Security
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long
API_KEY_HEADER=X-API-Key

# External Integrations
GITHUB_TOKEN=your-github-token
SLACK_BOT_TOKEN=your-slack-bot-token
SLACK_WEBHOOK_URL=your-slack-webhook-url

# Feature Flags
ENABLE_WEBSOCKETS=true
ENABLE_REAL_TIME_MONITORING=true
ENABLE_WORKFLOW_CACHING=true
ENABLE_AGENT_LEARNING=false
{
  "name": "@urnlabs/api",
  "version": "1.0.0",
  "description": "Urnlabs AI Agent Platform API",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "../../node_modules/.bin/tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "../../node_modules/.bin/tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@prisma/client": "^5.15.0",
    "@urnlabs/ai-agents": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "fastify": "^4.28.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "winston": "^3.13.0",
    "ioredis": "^5.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@urnlabs/config": "workspace:*",
    "eslint": "^8.56.0",
    "prisma": "^5.7.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}